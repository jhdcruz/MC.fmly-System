{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true
  },
  "settings": {
    "import/extensions": [
      ".js",
      ".ts"
    ]
  },
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-unused-vars": [
      "warn"
    ],
    "brace-style": "off",
    "no-console": "off",
    "no-var": "off",
    "one-var": "off",
    "import/no-anonymous-default-export": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
