{
  "name": "api",
  "version": "0.1.0",
  "license": "GPL-3.0-or-later",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "serve": "nodemon index.js --watch",
    "lint": "prettier \"**/**.{js,ts,jsx}\" --write && eslint \"**/**/*.{js,ts,jsx}\"",
    "lint-fix": "prettier \"**/**.{js,ts,jsx}\" --write && eslint \"**/**/*.{js,ts,jsx}\" --fix",
    "test": "echo \"Error: no test in server directory\" && exit 0"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "helmet": "4.1.1",
    "moesif-nodejs": "3.1.4",
    "mongoose": "5.10.9",
    "rollbar": "2.19.4"
  },
  "devDependencies": {
    "concurrently": "5.3.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.1",
    "nodemon": "2.0.6",
    "prettier": "2.1.2"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "settings": {
      "import/extensions": [
        ".js",
        ".jsx"
      ]
    },
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "brace-style": "off",
      "no-console": "off",
      "no-var": "off",
      "one-var": "off",
      "import/no-extraneous-dependencies": "off"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhdcruz/MC.fmly-System.git"
  },
  "keywords": [],
  "author": {
    "name": "Jhdcruz",
    "email": "jhdcruz00@gmail.com",
    "url": "https://github.com/jhdcruz"
  },
  "bugs": {
    "url": "https://github.com/jhdcruz/MC.fmly-System"
  },
  "homepage": "https://github.com/jhdcruz/MC.fmly-System"
}
