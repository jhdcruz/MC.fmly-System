{
  "version": 2,
  "public": false,
  "builds": [
    {
      "src": "/client/package.json",
      "use": "@now/static-build"
    },
    {
      "src": "/server/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/server/*",
      "dest": "/server/index.js"
    },
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": ".*",
      "dest": "/client/build/index.html"
    }
  ]
}
